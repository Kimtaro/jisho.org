					<div class="checkbox">
						<input type="checkbox" name="jy_only" id="kanji_s_jy" />
						<label for="kanji_s_jy">Jouyou kanji only</label>
					</div>
					<input type="submit" style="display:none" />
				</form>
			</div>
			<div class="result">
				<h3 class="status"></h3>
				<ul id="kanji_result_ul">
				</ul>
			</div>
